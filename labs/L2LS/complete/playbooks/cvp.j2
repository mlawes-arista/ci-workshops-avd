---
- name: Deploy to CloudVision
  {%- raw %}
  hosts: "{{ target_hosts }}"
  {% endraw -%}
  gather_facts: false

  tasks:

    - name: Deploy to Static Configuration Studio
      ansible.builtin.import_role:
        name: arista.avd.cv_deploy
      vars:
        cv_run_change_control: true
        cv_server: "{{ cvp }}-eos.topo.testdrive.arista.com"
